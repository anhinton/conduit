<?xml version="1.0"?>
<module xmlns="http://www.openapi.org/2014/" language="R">
  <description/>
  <input name="data">
    <internal symbol="data"/>
    <format formatType="text">R data frame</format>
  </input>
  <input name="clusters">
    <internal symbol="clusters"/>
    <format formatType="text">R kmeans object</format>
  </input>
  <source>
    <script><![CDATA[pdf("scattermatrix.pdf")
pairs(data, col = rainbow(3)[clusters$cluster])
dev.off()]]></script>
  </source>
  <output name="scattermatrix.pdf">
    <file ref="scattermatrix.pdf"/>
    <format formatType="text">PDF file</format>
  </output>
</module>
