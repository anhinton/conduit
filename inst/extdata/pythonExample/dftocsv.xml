<?xml version="1.0"?>
<module xmlns="http://www.openapi.org/2014" language="R">
  <description>create a CSV from a data frame</description>
  <input name="x">
    <internal symbol="x"/>
    <format formatType="text">R data frame</format>
  </input>
  <source>
    <script><![CDATA[write.csv(x, "x.csv", row.names = FALSE)]]></script>
  </source>
  <output name="x.csv">
    <file ref="x.csv"/>
    <format formatType="text">CSV file</format>
  </output>
</module>
